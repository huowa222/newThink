body {background:#fff}
h1,h2,h3,h4,h5,h6,p,form,fieldset,legend,button,input,textarea {margin:0;padding:0}
img,fieldset,hr,iframe {border:0}
a {text-decoration:none}
li {list-style:none}
body,input,button,textarea,select {font:normal 12px/1.667 Tahoma,Helvetica,Arial,"\5b8b\4f53",sans-serif;color:#333}
h1,h2,h3,h4,h5,h6,b {font-size:1em;font-weight:normal}
i,cite,em,var,dfn,address {font-style:normal}
a {text-decoration:none;outline:0}
a:link,a:visited {color:#3271C0}
a:hover {text-decoration:underline}
a,label {cursor:pointer}
.txt input,.tarea {background:#fff}
.txt input,option,textarea {background-color:#fff}
.txt {display:inline-block;*display:inline}
.txt input {height:17px;line-height:17px;margin:0 0 0.2em;padding:3px 3px 2px;border:1px solid #acacac;vertical-align:middle;-moz-border-radius:1px;-webkit-border-radius:1px;border-radius:1px;outline:0}

/*bdy*/
.bdy{width:960px;margin:0 auto;min-height:500px;_height:500px}
.bdy .c0{background:url(01.png) no-repeat;height:330px;position:absolute;top:50%;margin-top:-165px;width:960px}
.bdy .c0 .logForm{margin:150px 0 0 630px;width:250px}
.logForm .frm{margin:10px 0}
.logForm .frmT{width:70px;height:25px;line-height:24px;margin:0 0 -25px -70px;position:relative;left:70px;font-weight:bold;font-size:18px;font-family:"\5fae\8f6f\96c5\9ed1","\5b8b\4f53"}
.logForm .frmC {padding-left:70px}
.logForm .txt input{width:160px}
.logForm .frmSubmit .submit{background:url(02.png) no-repeat;width:123px;height:35px;text-align:center;line-height:35px;border:none;cursor:pointer}